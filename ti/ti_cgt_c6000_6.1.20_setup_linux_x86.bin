function validateWW(regForm) {
   var valid = 1;
   if(regForm.countryCodeEN) {
      if(trim(regForm.countryCodeEN.value) == "" || trim(regForm.countryCodeEN.value) == "-99" || trim(regForm.countryCodeEN.value) == "-100") {
         document.getElementById("newCountryDiv").className = "error";
         document.getElementById("newCountryDivErr").className = "errorMsg";
         document.getElementById("newCountryDivErr").innerHTML = "<p>Please select a country for signing up.</p>";
         valid = 0;
      }
      regForm.countryCode.value = regForm.countryCodeEN.value;
   }

   var email = trim(regForm.newmyTIEmailAddress.value);
   if(isValidEmail(email)) {
      if(email.substring(email.length-7,email.length) == "@ti.com" ||
                      email.substring(email.length-7,email.length) == ".ti.com") {
           document.getElementById("newEmailDiv").className = "error";
           document.getElementById("newEmailAddressErr").className = "errorMsg";
           document.getElementById("newEmailAddressErr").innerHTML = "<p>Your email address cannot be @ ti.com.</p>";
           valid = 0;
      }
   } else {
      document.getElementById("newEmailDiv").className = "error";
      document.getElementById("newEmailAddressErr").className = "errorMsg";
      document.getElementById("newEmailAddressErr").innerHTML = "<p>We can't recognize your email address; please check it for accuracy</p>";
      valid = 0;
   }
   regForm.emailId.value = regForm.newmyTIEmailAddress.value;

   var pwd = trim(regForm.newmyTIPassword.value);
   if(isValidPassword(pwd) == 'false') {
      document.getElementById("newPasswordDiv").className = "error";
      document.getElementById("newPasswordErr").className = "errorMsg";
      document.getElementById("newPasswordErr").innerHTML = "<p>Your password must contain at least 6 characters; please select a different password</p>";
      valid = 0;
    } else if (isValidPassword(pwd) == 'n-ascii') {
       document.getElementById("newPasswordDiv").className = "error";
       document.getElementById("newPasswordErr").className = "errorMsg";
       document.getElementById("newPasswordErr").innerHTML = "<p>Please use only ASCII characters</p>";
       valid = 0;
    }
    regForm.password.value = regForm.newmyTIPassword.value;

    if(trim(regForm.newmyTIConfirmPassword.value) != pwd) {
       document.getElementById("newConfirmPasswordDiv").className = "error";
       document.getElementById("newConfirmPasswordErr").className = "errorMsg";
       document.getElementById("newConfirmPasswordErr").innerHTML = "<p>This password must match the password above; please retype</p>";
       valid = 0;
    }
    regForm.passwordConfirm.value = regForm.newmyTIConfirmPassword.value;

    if(trim(regForm.newmyTIFirstName.value) == "") {
       document.getElementById("newFirstNameDiv").className = "error";
       document.getElementById("newFirstNameErr").className = "errorMsg";
       document.getElementById("newFirstNameErr").innerHTML = "<p>Please provide this information</p>";
       valid = 0;
    } else if(regForm.newmyTIFirstName.value.indexOf('|') >= 0) {
       document.getElementById("newFirstNameDiv").className = "error";
       document.getElementById("newFirstNameErr").className = "errorMsg";
       document.getElementById("newFirstNameErr").innerHTML = "<p>Please do not use the "|" (vertical bar) character in name</p>";
       valid = 0;
    }
    regForm.firstName.value = regForm.newmyTIFirstName.value;

    if(trim(regForm.newmyTILastName.value) == "") {
       document.getElementById("newLastNameDiv").className = "error";
       document.getElementById("newLastNameErr").className = "errorMsg";
       document.getElementById("newLastNameErr").innerHTML = "<p>Please provide this information</p>";
       valid = 0;
    } else if(regForm.newmyTILastName.value.indexOf('|') >= 0) {
       document.getElementById("newLastNameDiv").className = "error";
       document.getElementById("newLastNameErr").className = "errorMsg";
       document.getElementById("newLastNameErr").innerHTML = "<p>Please do not use the "|" (vertical bar) character in name</p>";
       valid = 0;
    }
    regForm.lastName.value = regForm.newmyTILastName.value;

    if(trim(regForm.newmyTICompany.value) == "") {
       document.getElementById("newCompanyDiv").className = "error";
       document.getElementById("newCompanyErr").className = "errorMsg";
       document.getElementById("newCompanyErr").innerHTML = "<p>Please provide this information</p>";
       valid = 0;
    } else if(regForm.newmyTICompany.value.indexOf('|') >= 0) {
       document.getElementById("newCompanyDiv").className = "error";
       document.getElementById("newCompanyErr").className = "errorMsg";
       document.getElementById("newCompanyErr").innerHTML = '<p>Please do not use the "|" (vertical bar) character in name</p>';
       valid = 0;
    }
    regForm.companyName.value = regForm.newmyTICompany.value;

    if(regForm.newmyTIRememberMe.checked == true) {
       regForm.rememberMe.value = 'true';
    }
    if(regForm.newmyTIEmailSignup.checked == true) {
       regForm.subscribe.value = 'true';
    }
    regForm.action = 'https://focus.ti.com/tipass/docs/register.tsp?';
    return valid;
}

